{
    "name": "Сбор данных менеджерам RedLab",
    "description": "Автоматическая отправка данных о ходе работ.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [ "tabs" ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "icons": {
        "16": "icons/redlab-16.png",
        "32": "icons/redlab-32.png",
        "48": "icons/redlab-48.png",
        "128": "icons/redlab-128.png"
    }
    
}
